<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    <url>
        <loc>{{ urlIndex }}</loc>
        <changefreq>monthly</changefreq>
        <priority>1</priority>
    </url>
    {% for entitySitemapData in entityList %}
    <url>
        <loc>{% autoescape false %}{{ urlIndex }}{{ entitySitemapData.loc }}{% endautoescape %}</loc>
        <changefreq>{{ entitySitemapData.changefreq }}</changefreq>
        <priority>{{ entitySitemapData.priority }}</priority>
    </url>
    {% endfor %}
</urlset>
