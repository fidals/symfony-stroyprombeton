			<li><a href="{{ path('app_catalog_gbi_visual') }}">Каталог продукции</a></li>
			<li><a href="{{ path('app_main_staticpage', { 'alias': 'about' }) }}">О компании</a></li>
			<li><a href="{{ path('app_main_staticpage', { 'alias': 'proizvodstvo-gbi' }) }}">Производство</a></li>
			<li><a href="{{ path('app_main_territory_map') }}">Наши объекты</a></li>
			<li><a href="{{ path('app_main_staticpage', { 'alias': 'delivery' }) }}">Доставка</a></li>
			<li><a href="{{ path('app_main_staticpage', { 'alias': 'contacts' }) }}">Контактная информация</a></li>
			<li>
				<div id="sticky-wrapper" class="basket-sticky-wrapper basket-sticky">
					<div class="basket basket-sticky">
						<div class="menu-basket">
							{% include 'AppCatalogBundle:Cart:cart.html.twig' %}
						</div>
					</div>
				</div>
			</li>
		</ul>
	</div>
	<div class="page-content">
