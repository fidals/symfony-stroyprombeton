						<table>
							<tr>
								<td class="search-form-title">Поиск в каталоге продукции:</td>
								<td class="search-form-input">
									<input id="search_condition" type="text" value="{% if searchCondition|default %}{{ searchCondition }}{% else %}Запрос для поиска ...{% endif %}">
								</td>
								<td class="search-form-submit">
									<input class="btn-gray btn-search-action" id="btn-search-action" type="button" value="Найти">
								</td>
							</tr>
						</table>
					</div>
					<div class="primary-content">
						{% block content %}{% endblock %}
					</div>
				</div>
			</div>
		</div>

