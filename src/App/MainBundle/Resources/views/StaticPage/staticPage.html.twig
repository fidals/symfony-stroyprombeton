{% extends "AppMainBundle:Default:layout.html.twig" %}

{% block title %}
	{{ staticPage.getTitle() }}
{% endblock %}

{% block description %}
	{{ staticPage.getDescription() }}
{% endblock %}

{% block keywords %}
	{{ staticPage.getKeywords() }}
{% endblock %}

{% block content %}
	{% autoescape false %}
		<h1>{{ staticPage.getTitle() }}</h1>
		{{ include(template_from_string(staticPage.getText())) }}
	{% endautoescape %}
{% endblock %}