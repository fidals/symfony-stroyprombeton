<h4 class="entity-form-title toggle-block js-toggle-trigger">
  SEO <i class="fa fa-caret-down"></i>
</h4>

<div class="toggle-section js-toggle-target">
  <div class="form-group">
    <div class="checkbox">
      <label>{{ form_widget(form.isActive) }} Активно</label>
    </div>
  </div>

  <div class="form-group">
    <div class="input-group">
      <div class="input-group-addon">Title</div>
      {{ form_widget(form.title, {'attr': {'class': 'form-control'}}) }}
    </div>
  </div>

  <div class="form-group">
    <div class="input-group">
      <div class="input-group-addon">H1</div>
      {{ form_widget(form.H1, {'attr': {'class': 'form-control'}}) }}
    </div>
  </div>

  <div class="form-group">
    <label class="control-label sr-only" for="input-keywords">Keywords</label>
    <div class="input-group">
      <div class="input-group-addon">Keywords</div>
      {{ form_widget(form.keywords, {'attr': {'class': 'form-control'}}) }}
    </div>
  </div>

  <div class="form-group">
    <label class="control-label sr-only" for="input-description">Descripton</label>
    <div class="input-group">
      <div class="input-group-addon">Descripton</div>
      {{ form_widget(form.description, {'attr': {'class': 'form-control'}}) }}
    </div>
  </div>
</div>
